SELECT k.KundID, k.Pnr, k.Namn, k.Adress, k.Postnr, k.Ort, k.Hemtelefon, k.Arbetstelefon, k.Mobiltelefon, k.Epostadress, k.Alder
FROM dw.Kund k
-- Utan Maxfonder
WHERE NOT EXISTS (SELECT 1 FROM dw.MaxfonderKund mk WHERE mk.KundNr = k.Pnr)
-- Utan Kundmöte sedan 2016-01-01
AND NOT EXISTS (SELECT 1 FROM dw.KundMote km WHERE km.KundID = k.KundID AND km.SenasteMote > DateAdd(yy, -2, GetDate()))
AND Ort = 'Malmö'
OR Ort = 'Arlöv'
OR Ort = 'Bunkeflostrand'
OR Ort = 'Klagshamn'
OR Ort = 'Limhamn'
OR Ort = 'Oxie'
OR Ort = 'Tygelsjö'
OR Ort = 'Vellinge'
OR Ort = 'Vintrie'
OR Ort = 'Lund'
OR Ort = 'Svedala'
OR Ort = 'Staffanstorp'
OR Ort = 'Burlöv'

SELECT km.KundID, km.SenasteMote, km.NastaMote
FROM dw.KundMote km
JOIN dw.Kund k ON km.KundiD = k.KundID
WHERE SenasteMote > DateAdd(yy, -2, GetDate())
ORDER BY SenasteMote DESC



SELECT
	k.KundID, k.Pnr, k.Namn, k.Adress,k.Postnr,
	k.Ort, k.Hemtelefon, k.Arbetstelefon,k.Mobiltelefon,
	k.Epostadress, k.Alder, kap.Kapital,kap.Trad, kap.Fond,
	kap.AktivFond, kap.Indexfond, kap.Rantefond
FROM dw.Kund k
JOIN sox.kapital kap ON k.KundID = kap.KundID
	-- Utan Maxfonder
	WHERE NOT EXISTS (SELECT 1 FROM dw.MaxfonderKund mk WHERE mk.KundNr = k.Pnr)
	-- Utan Kundmöte sedan 2 år bakåt
	AND NOT EXISTS (SELECT 1 FROM dw.KundMote km WHERE km.KundID = k.KundID AND km.SenasteMote > DateAdd(yy, -2, GetDate()))
		AND Ort = 'Malmö'			AND kap.Navigera IS NULL
		OR Ort = 'Arlöv'			AND kap.Navigera IS NULL
		OR Ort = 'Bunkeflostrand'	AND kap.Navigera IS NULL
		OR Ort = 'Klagshamn'		AND kap.Navigera IS NULL
		OR Ort = 'Limhamn'			AND kap.Navigera IS NULL
		OR Ort = 'Oxie'				AND kap.Navigera IS NULL
		OR Ort = 'Tygelsjö'			AND kap.Navigera IS NULL
		OR Ort = 'Vellinge'			AND kap.Navigera IS NULL
		OR Ort = 'Vintrie'			AND kap.Navigera IS NULL
		OR Ort = 'Lund'				AND kap.Navigera IS NULL
		OR Ort = 'Svedala'			AND kap.Navigera IS NULL
		OR Ort = 'Staffanstorp'		AND kap.Navigera IS NULL
		OR Ort = 'Burlöv'			AND kap.Navigera IS NULL
	ORDER BY Kapital DESC
	
	
	
	
  SELECT
	km.SenasteMote,
	k.Pnr, k.Namn, k.Adress,k.Postnr,
	k.Ort, k.Hemtelefon, k.Arbetstelefon,k.Mobiltelefon,
	k.Epostadress, k.Alder, kap.Kapital, kap.Trad, kap.Fond,
	kap.AktivFond, kap.Indexfond, kap.Rantefond
  FROM dw.Radgivare r
  LEFT JOIN dw.Kund k ON r.RadgivarID = k.RadgivarID
  LEFT JOIN dw.KundMote km ON k.KundID = km.KundID
  JOIN sox.kapital kap ON k.KundID = kap.KundID
  AND Roll = 'Slutat'
  WHERE NOT EXISTS (SELECT 1 FROM dw.MaxfonderKund mk WHERE mk.KundNr = k.Pnr)
  		AND Ort = 'Malmö'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Arlöv'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Bunkeflostrand'	AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Klagshamn'		AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Limhamn'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Oxie'				AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Tygelsjö'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Vellinge'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Vintrie'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Lund'				AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Svedala'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Staffanstorp'		AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
		OR Ort = 'Burlöv'			AND kap.Navigera IS NULL	  AND Roll = 'Slutat'		
	ORDER BY Kapital DESC

==============================================================================================================
--SENASTE

SELECT DISTINCT
	r.Team, k.Pnr, k.Namn, kf.Namn AS Foretagsnamn, k.Adress,
	k.Postnr, k.Ort, k.Hemtelefon, k.Arbetstelefon,
	k.Mobiltelefon, k.Epostadress, k.Alder
FROM dw.kund k
LEFT JOIN dw.Radgivare r On k.RadgivarID = r.RadgivarID
LEFT JOIN dw.Kundforetag kf ON k.KundforetagID = kf.KundforetagID
LEFT JOIN sox.kapital kap ON k.KundID = kap.KundID
WHERE r.Team = 'Team Lindberg'
AND kf.Namn LIKE '%slutat%' AND Ort = 'Malmö'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Lund'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Arlöv'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Bunkeflostrand'	AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Klagshamn'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Limhamn'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Oxie'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Tygelsjö'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Vellinge'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Vintrie'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Svedala'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Staffanstorp'	AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Burlöv'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Trelleborg'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Höllviken'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Falsterbo'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Skanör'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Sjöbo'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Lomma'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Alnarp'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Bjärred'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Löddeköpinge'	AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Bara'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Klågerup'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Södra Sandby'	AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Dalby'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Genarp'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Veberöd'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Blentarp'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Skurup'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Västra Ingelstad'AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Östra Grevie'	AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Anderslöv'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Kävlinge'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Örtofta'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Eslöv'			AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Teckomatorp'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Landskrona'		AND r.Team = 'Team Lindberg'
OR kf.Namn LIKE '%slutat%'  AND Ort = 'Svalöv'			AND r.Team = 'Team Lindberg'
ORDER BY Ort
